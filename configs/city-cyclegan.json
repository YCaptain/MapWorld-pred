{
    "name": "City-CycleGAN",
    "n_gpu": 1,
    "log_config": "src/logger/logger_config.json",
    "save_dir": "saved/",
    "arch": {
         "type": "CycleGANOptions",
         "args": {
         }
    },
    "data_loader": {
        "type": "SpaceNetDataLoader",
        "args":{
            "data_type": "building",
            "data_name": [
                "AOI_2_Vegas_Train",
                "AOI_3_Paris_Train",
                "AOI_4_Shanghai_Train",
                "AOI_5_Khartoum_Train"
            ],
            "data_dir": "data/FYPData/spacenet/buildings",
            "batch_size": 25,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 4,
            "scales": [0.5, 0.75, 1.0, 1.25, 1.5],
            "crop_size": 325
        }
    }
}