{
    "name": "DeepLabTest",
    "n_gpu": 1,
    "log_config": "src/logger/logger_config.json",
    "arch": {
         "type": "DeepLabV3Plus",
         "args": {
             "n_classes": 2,
             "n_blocks": [3, 4, 23, 3],
             "atrous_rates": [6, 12, 18],
             "multi_grids": [1, 2, 4],
             "output_stride": 16
         }
    },
    "data_loader": {
        "type": "SpaceNetDataLoader",
        "args":{
            "data_name": "AOI_3_Paris_Train",
            "data_dir": "data/FYPData/spacenet",
            "batch_size": 128,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 2,
            "colors": {
                "building": [255, 0, 0],
                "road": [0, 0, 255],
                "ignore": [0, 0, 0]
            }
        }
    },
    "trainer": {
      "save_dir": "saved/"
    }
}