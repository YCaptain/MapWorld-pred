{
   "name": "GeoPreprocessorTest",
   "n_gpu": 1,
   "log_config": "src/logger/logger_config.json",
   "data_loader": {
        "type": "GeoDataLoader",
        "args":{
            "data_name": "AOI_5_Khartoum_Train",
            "data_dir": "data/FYPData/spacenet",
            "batch_size": 20,
            "shuffle": true,
            "validation_split": 0.1,
